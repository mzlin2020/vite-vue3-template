{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "plugin:prettier/recommended",
        "eslint-config-prettier"
    ],
    "plugins": ["vue", "prettier"],
    "rules": {
        "vue/multi-word-component-names": "off",
        "no-console": [
            "warn",
            {
                "allow": ["warn", "error"]
            }
        ],
        "no-debugger": "warn"
    }
}
